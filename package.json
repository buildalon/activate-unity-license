{
  "name": "activate-unity-license",
  "version": "1.0.4",
  "description": "A GitHub Action to activate a Unity Game Engine license for CI/CD workflows.",
  "author": "buildalon",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/buildalon/activate-unity-license.git"
  },
  "bugs": {
    "url": "https://github.com/buildalon/activate-unity-license/issues"
  },
  "homepage": "https://github.com/buildalon/activate-unity-license",
  "main": "dist/index.js",
  "keywords": [
    "unity",
    "license",
    "activation",
    "github",
    "action"
  ],
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/exec": "^1.1.1",
    "@actions/glob": "^0.4.0"
  },
  "devDependencies": {
    "@types/node": "^22.2.0",
    "@vercel/ncc": "^0.34.0",
    "shx": "^0.3.4",
    "typescript": "^5.5.4"
  },
  "scripts": {
    "build": "npm run clean && npm run bundle",
    "bundle": "ncc build src/index.ts -o dist --source-map --license licenses.txt",
    "clean": "npm install && shx rm -rf dist/ out/ node_modules/ && npm ci"
  }
}